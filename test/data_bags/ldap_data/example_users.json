{
    "sudoers": {
        "%admins": {
            "option": [
                "!authenticate"
            ],
            "host": "ALL",
            "command": "ALL",
            "run_as_user": "ALL"
        }, 
        "%dummy": {
            "option": [
                "!authenticate"
            ],
            "host": [
              "10.10.10.10",
              "10.10.10.11"
            ],
            "command": "ALL",
            "run_as_user": "ALL"
        }, 
        "defaults": {
            "option": [
                "env_keep+=SSH_AUTH_SOCK", 
                "requiretty", 
                "!visiblepw", 
                "always_set_home", 
                "env_reset", 
                "env_keep+=\"COLORS DISPLAY HOSTNAME HISTSIZE INPUTRC KDEDIR LS_COLORS\"", 
                "env_keep+=\"MAIL PS1 PS2 QTDIR USERNAME LANG LC_ADDRESS LC_CTYPE\"", 
                "env_keep+=\"LC_COLLATE LC_IDENTIFICATION LC_MEASUREMENT LC_MESSAGES\"", 
                "env_keep+=\"LC_MONETARY LC_NAME LC_NUMERIC LC_PAPER LC_TELEPHONE\"", 
                "env_keep+=\"LC_TIME LC_ALL LANGUAGE LINGUAS _XKB_CHARSET XAUTHORITY\"", 
                "secure_path=/sbin:/bin:/usr/sbin:/usr/bin"
            ]
        }
    }, 
    "id": "ldap_user_bag_example", 
    "groups": {
        "admins": {
            "gid_number": 1001, 
            "members": [
                "vagrantldap"
            ]
        },
        "dummy": {
            "gid_number": 1002, 
            "members": [
                "vagrantldap2"
            ]
        }
     }, 
    "users": {
        "vagrantldap": {
            "gid_number": 100, 
            "surname": "LDAPUser", 
            "firstname": "Vagrant", 
            "ssh_pubkey": "ssh-rsa  vagrant insecure public key", 
            "password": "vagrant",
            "uid_number": 900,
            "mail": "test@example.com"
        },
        "vagrantldap2": {
            "gid_number": 100,
            "surname": "LDAPUser",
            "firstname": "Vagrant2",
            "uid_number": 901,
            "ssh_pubkey": [
              "ssh-rsa  vagrant insecure public key",
              "ssh-rsa  vagrant insecure public key fake"
            ],
            "password": "vagrant",
            "mail": "test@example.com"
        },
        "vagrantldap3": {
            "gid_number": 100,
            "surname": "LDAPUser",
            "firstname": "Vagrant3",
            "uid_number": 902,
            "password": "vagrant",
            "mail": "test@example.com",
            "action": "delete"
        }
    }
}
